return[[unRotate(0)],[unRotate(180)]];}
if(MakerJs.round(x-c2.radius)>c1.radius){return null;}
if(MakerJs.round(x+c2.radius)<c1.radius){return null;}
if(MakerJs.round(x-c2.radius)<-c1.radius){return null;}
function bothAngles(oneAngle){return[unRotate(oneAngle),unRotate(MakerJs.angle.mirror(oneAngle,false,true))];}
