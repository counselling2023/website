function getChainBezierRange(c){var endLinks=[c.links[0],c.links[c.links.length-1]];if(endLinks[0].walkedPath.pathContext.bezierData.startT>endLinks[1].walkedPath.pathContext.bezierData.startT){MakerJs.chain.reverse(c);endLinks.reverse();}
