'||obj.text().trim()!=''){var last_error_type=null;var obj_val=(obj.val().trim()!='')?obj.val():obj.text();var err_flag=true;$.each(_self.actCheckTypes,function(i,o){var is_not_valid=o.regEx.test(obj_val);if(is_not_valid){_self.validation_error_types.push(i);_self.validation_error_msg.push(_self.actCheckMsg[i]);if(typeof _self.settings.error_callback==="function"){_self.settings.error_callback.apply(obj,_self.validation_error_msg);}
