function displaySocialPopup(dataMsg){if(typeof dataMsg.social_data=="object"&&typeof get_social_popup_data=="function"){var t=parseInt(dataMsg.social_data.popup_appear_time),i=get_social_popup_data(dataMsg.social_data),n=dataMsg.social_data.popup_opener_name;if(!$("#loadere").length){$('body').append('<div id="loadere" style="display:none;"><div id="loadere_new1"></div><div id="loadere_new2"></div></div>');}
var modal_type=dataMsg.social_data.modal_type;if(modal_type=='ouibounce'){$('body .main-container').append(i);if($("#"+dataMsg.social_data.popup_opener_name).length>0){var aggressive=true;var ouibounce_opts={aggressive:true,callback:function(){if(typeof initSocialTargets=="function"){initSocialTargets();}
